string = input("STRING> ")
string = str(string)
print("Enter four numbers to slice the string")
a = int(input("A> ")) + 1
b = int(input("B> "))
c = int(input("C> ")) + 1
d = int(input("D> "))

print("OUTPUT", string[a:b], string[c:d])
